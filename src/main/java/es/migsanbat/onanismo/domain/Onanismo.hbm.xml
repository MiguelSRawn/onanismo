<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
 
<hibernate-mapping package="es.migsanbat.onanismo.domain">
 
    <class name="Onanismo" table="ONANISMO">
        <id name="id" column="ONANISMO_ID">
            <generator class="increment"/>
        </id>
        <property name="title" column="ONANISMO_TITLE"/>
    </class>
 
</hibernate-mapping>